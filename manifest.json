        },
        {
            "matches": [
                "*://*.zeit.de/*"
            ],
            "js": [
                "websites/zeit-de.js",
                "common.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "websites/arstechnica-com.js"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "declarativeContent",
        "http://*/*",
        "https://*/*",
        "storage"
    ],
    "-ms-preload": {
        "backgroundScript": "assets/edge/backgroundScriptsAPIBridge.js",
        "contentScript": "assets/edge/contentScriptsAPIBridge.js"
    }
}